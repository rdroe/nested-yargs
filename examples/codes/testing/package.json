{
    "name": "my-bootstrapped",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "description": "a browser app that allows yargs use in an xterm-js terminal",
    "workspaces": [
        "../nyargs"
    ],
    "keywords": [
        "yargs",
        "node",
        "browser",
        "cli"
    ],
    "dependencies": {
        "@testing-library/dom": "^8.18.1",
        "@testing-library/user-event": "^14.4.3",
        "concurrently": "^7.3.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "http-server": "^14.1.0",
        "nodemon": "^2.0.16",
        "nyargs": "workspace:*",
        "swipl": "^1.0.6",
        "ts-node": "^10.9.1",
        "xterm": "^4.18.0",
        "yargs": "^17.5.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/isomorphic-fetch": "^0.0.36",
        "@types/yargs": "^17.0.10",
        "esbuild": "^0.14.38",
        "esbuild-plugin-copy": "^1.3.0",
        "typescript": "^4.6.4"
    },
    "scripts": {
        "ts-dev": "tsc -b && yarn tsc -w",
        "ts-build": "tsc",
        "js-build": "node build.js",
        "start": "yarn http-server",
        "serve": "ts-node src/serve.ts",
        "mon-br": "nodemon -w \"src/**/*\" -e ts,js --exec \"yarn ts-build && yarn js-build && yarn start\""
    },
    "packageManager": "yarn@3.2.3"
}
